{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc",
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "start": "yarn clean && tsc && node -r dotenv/config dist",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "tscCheck": "yarn tsc --noEmit",
    "lint:all": "yarn format && yarn lint && yarn tscCheck",
    "test": "jest --clear-cache && cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "yarn test --watch",
    "prettier": "prettier --check **/*.{js,ts,json,md}",
    "prettier:fix": "prettier --write **/*.{js,ts,json,md}",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build -d"
  },
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "mongodb-memory-server": "^8.0.2",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "email-validator": "^2.0.4",
    "express": "^4.16.4",
    "express-openapi-validator": "^4.13.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.2.0",
    "mongoose": "^6.0.13",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.30",
    "swagger-ui-express": "^4.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "rootDir": "tests"
  }
}
